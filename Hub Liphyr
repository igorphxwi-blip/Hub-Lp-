    setclipboard("https://github.com/SEU_USUARIO/SEU_REPO")
end)

-- Verifica√ß√£o da Key
VerifyButton.MouseButton1Click:Connect(function()
    local inputKey = KeyInput.Text
    
    if inputKey == "" then
        KeyStatus.Text = "‚ùå Digite uma key!"
        KeyStatus.TextColor3 = Color3.fromRGB(255, 100, 100)
        return
    end
    
    KeyStatus.Text = "üîÑ Verificando key..."
    KeyStatus.TextColor3 = Color3.fromRGB(255, 255, 100)
    VerifyButton.Text = "üîÑ VERIFICANDO..."
    
    -- Validar key
    local isValid, message = validateKey(inputKey)
    
    if isValid then
        KeyStatus.Text = "‚úÖ " .. message
        KeyStatus.TextColor3 = Color3.fromRGB(100, 255, 100)
        VerifyButton.Text = "‚úÖ KEY V√ÅLIDA!"
        
        -- Salvar key
        saveKey(inputKey)
        
        -- Aguardar e iniciar script principal
        wait(2)
        KeyFrame:Destroy()
        loadMainScript()
    else
        KeyStatus.Text = "‚ùå " .. message
        KeyStatus.TextColor3 = Color3.fromRGB(255, 100, 100)
        VerifyButton.Text = "‚ùå KEY INV√ÅLIDA"
        wait(2)
        VerifyButton.Text = "‚úÖ VERIFICAR KEY"
    end
end)

-- Verificar se j√° tem key salva
local savedKey = loadSavedKey()
if savedKey then
    KeyStatus.Text = "üîÑ Verificando key salva..."
    local isValid, message = validateKey(savedKey)
    
    if isValid then
        KeyStatus.Text = "‚úÖ Key v√°lida encontrada!"
        KeyStatus.TextColor3 = Color3.fromRGB(100, 255, 100)
        wait(2)
        KeyFrame:Destroy()
        loadMainScript()
        return
    else
        -- Key expirou ou inv√°lida, deletar
        if delfile then delfile(keyFile) end
        KeyStatus.Text = "‚ö†Ô∏è Key salva expirou, digite uma nova"
        KeyStatus.TextColor3 = Color3.fromRGB(255, 200, 100)
    end
end

-- SCRIPT PRINCIPAL (carrega ap√≥s verifica√ß√£o)
function loadMainScript()
    local ScreenGui = Instance.new("ScreenGui")
    ScreenGui.Name = "RainbowAutoJoinUI"
    ScreenGui.Parent = game:GetService("CoreGui")

    local MainFrame = Instance.new("Frame")
    MainFrame.Size = UDim2.new(0, 350, 0, 220)
    MainFrame.Position = UDim2.new(0.5, -175, 0.4, -110)
    MainFrame.BackgroundColor3 = Color3.fromRGB(20, 20, 25)
    MainFrame.BorderSizePixel = 3
    MainFrame.BorderColor3 = Color3.fromRGB(255, 0, 255)
    MainFrame.Active = true
    MainFrame.Draggable = true
    MainFrame.Parent = ScreenGui

    -- Bot√£o de fechar na janela principal
    local CloseButton = Instance.new("TextButton")
    CloseButton.Size = UDim2.new(0, 25, 0, 25)
    CloseButton.Position = UDim2.new(1, -30, 0, 5)
    CloseButton.BackgroundColor3 = Color3.fromRGB(255, 50, 50)
    CloseButton.Text = "‚úñ"
    CloseButton.TextColor3 = Color3.fromRGB(255, 255, 255)
    CloseButton.Font = Enum.Font.GothamBold
    CloseButton.TextSize = 12
    CloseButton.Parent = MainFrame

    local CloseCorner = Instance.new("UICorner")
    CloseCorner.CornerRadius = UDim.new(0.5, 0)
    CloseCorner.Parent = CloseButton

    CloseButton.MouseButton1Click:Connect(function()
        MainFrame.Visible = false
    end)

    local MainCorner = Instance.new("UICorner")
    MainCorner.CornerRadius = UDim.new(0, 15)
    MainCorner.Parent = MainFrame

    local Title = Instance.new("TextLabel")
    Title.Size = UDim2.new(1, 0, 0, 50)
    Title.Position = UDim2.new(0, 0, 0, 0)
    Title.BackgroundColor3 = Color3.fromRGB(15, 15, 20)
    Title.Text = "LIPHYR AUTO JOIN"
    Title.TextColor3 = Color3.fromRGB(255, 255, 255)
    Title.Font = Enum.Font.GothamBold
    Title.TextSize = 16
    Title.TextXAlignment = Enum.TextXAlignment.Center
    Title.TextYAlignment = Enum.TextYAlignment.Center
    Title.Parent = MainFrame

    local TitleCorner = Instance.new("UICorner")
    TitleCorner.CornerRadius = UDim.new(0, 15)
    TitleCorner.Parent = Title

    -- Label do TikTok
    local TikTokLabel = Instance.new("TextLabel")
    TikTokLabel.Size = UDim2.new(1, 0, 0, 25)
    TikTokLabel.Position = UDim2.new(0, 0, 0, 50)
    TikTokLabel.BackgroundTransparency = 1
    TikTokLabel.Text = "ttk@liphyr"
    TikTokLabel.TextColor3 = Color3.fromRGB(255, 20, 147)
    TikTokLabel.Font = Enum.Font.GothamBold
    TikTokLabel.TextSize = 14
    TikTokLabel.TextXAlignment = Enum.TextXAlignment.Center
    TikTokLabel.Parent = MainFrame

    -- Efeitos Rainbow
    spawn(function()
        local hue = 0
        while MainFrame.Parent do
            hue = (hue + 1) % 360
            local r = math.floor(255 * (1 + math.sin(math.rad(hue))) / 2)
            local g = math.floor(255 * (1 + math.sin(math.rad(hue + 120))) / 2)
            local b = math.floor(255 * (1 + math.sin(math.rad(hue + 240))) / 2)
            MainFrame.BorderColor3 = Color3.fromRGB(r, g, b)
            wait(0.1)
        end
    end)

    spawn(function()
        local hue = 0
        while Title.Parent do
            hue = (hue + 2) % 360
            local r = math.floor(255 * (1 + math.sin(math.rad(hue))) / 2)
            local g = math.floor(255 * (1 + math.sin(math.rad(hue + 120))) / 2)
            local b = math.floor(255 * (1 + math.sin(math.rad(hue + 240))) / 2)
            Title.TextColor3 = Color3.fromRGB(r, g, b)
            wait(0.15)
        end
    end)

    -- Resto do c√≥digo do Auto Join (suas fun√ß√µes originais)
    local function formatMoney(num)
        return string.format("%.1fM", num / 1000000)
    end

    local function parseInput(str)
        local num = tonumber(str)
        if num then
            return math.clamp(num * 1000000, 1000000, 10000000)
        end
        return nil
    end

    local hwid = "unknown_hwid"
    pcall(function()
        hwid = game:GetService("RbxAnalyticsService"):GetClientId()
    end)

    local cfgFile = "rainbow_autojoin_"..hwid..".json"

    local function loadConfig()
        if isfile and isfile(cfgFile) then
            local success, data = pcall(function()
                return HttpService:JSONDecode(readfile(cfgFile))
            end)
            if success and type(data) == "table" then
                return data
            end
        end
        return { MinMS = 1000000 }
    end

    local function saveConfig(cfg)
        if writefile then
            writefile(cfgFile, HttpService:JSONEncode(cfg))
        end
    end

    local config = loadConfig()
    local MinMS = config.MinMS or 1000000

    local AutoJoinEnabled = false
    local Toggle = Instance.new("TextButton")
    Toggle.Size = UDim2.new(1, -20, 0, 40)
    Toggle.Position = UDim2.new(0, 10, 0, 80)
    Toggle.BackgroundColor3 = Color3.fromRGB(30, 30, 40)
    Toggle.Text = "üöÄ Auto Join: OFF üöÄ"
    Toggle.TextColor3 = Color3.fromRGB(255, 100, 100)
    Toggle.Font = Enum.Font.GothamBold
    Toggle.TextSize = 16
    Toggle.Parent = MainFrame

    local ToggleCorner = Instance.new("UICorner")
    ToggleCorner.CornerRadius = UDim.new(0, 10)
    ToggleCorner.Parent = Toggle

    spawn(function()
        local hue = 180
        while Toggle.Parent do
            if AutoJoinEnabled then
                hue = (hue + 3) % 360
                local r = math.floor(255 * (1 + math.sin(math.rad(hue))) / 2)
                local g = math.floor(255 * (1 + math.sin(math.rad(hue + 120))) / 2)
                local b = math.floor(255 * (1 + math.sin(math.rad(hue + 240))) / 2)
                Toggle.BackgroundColor3 = Color3.fromRGB(math.max(30, r/3), math.max(30, g/3), math.max(40, b/3))
                Toggle.TextColor3 = Color3.fromRGB(r, g, b)
            end
            wait(0.1)
        end
    end)

    Toggle.MouseButton1Click:Connect(function()
        AutoJoinEnabled = not AutoJoinEnabled
        Toggle.Text = AutoJoinEnabled and "üåü Auto Join: ON üåü" or "üöÄ Auto Join: OFF üöÄ"
        if not AutoJoinEnabled then
            Toggle.BackgroundColor3 = Color3.fromRGB(30, 30, 40)
            Toggle.TextColor3 = Color3.fromRGB(255, 100, 100)
        end
    end)

    -- Min Label
    local MinLabel = Instance.new("TextLabel")
    MinLabel.Size = UDim2.new(1, -20, 0, 30)
    MinLabel.Position = UDim2.new(0, 10, 0, 130)
    MinLabel.BackgroundTransparency = 1
    MinLabel.Text = "üí∞ Min M/s: " .. formatMoney(MinMS) .. " üí∞"
    MinLabel.TextColor3 = Color3.fromRGB(255, 215, 0)
    MinLabel.Font = Enum.Font.GothamBold
    MinLabel.TextSize = 14
    MinLabel.Parent = MainFrame

    -- Min TextBox
    local MinBox = Instance.new("TextBox")
    MinBox.Size = UDim2.new(1, -20, 0, 30)
    MinBox.Position = UDim2.new(0, 10, 0, 165)
    MinBox.BackgroundColor3 = Color3.fromRGB(40, 40, 50)
    MinBox.Text = tostring(MinMS/1000000)
    MinBox.TextColor3 = Color3.fromRGB(255, 255, 255)
    MinBox.Font = Enum.Font.Gotham
    MinBox.TextSize = 14
    MinBox.ClearTextOnFocus = false
    MinBox.PlaceholderText = "Digite o valor m√≠nimo..."
    MinBox.Parent = MainFrame

    local MinBoxCorner = Instance.new("UICorner")
    MinBoxCorner.CornerRadius = UDim.new(0, 8)
    MinBoxCorner.Parent = MinBox

    -- Status Label
    local StatusLabel = Instance.new("TextLabel")
    StatusLabel.Size = UDim2.new(1, -20, 0, 20)
    StatusLabel.Position = UDim2.new(0, 10, 0, 200)
    StatusLabel.BackgroundTransparency = 1
    StatusLabel.Text = "‚ú® Status: Aguardando... ‚ú®"
    StatusLabel.TextColor3 = Color3.fromRGB(150, 150, 255)
    StatusLabel.Font = Enum.Font.Gotham
    StatusLabel.TextSize = 12
    StatusLabel.Parent = MainFrame

    MinBox.FocusLost:Connect(function()
        local val = parseInput(MinBox.Text)
        if val then
            MinMS = val
            MinLabel.Text = "üí∞ Min M/s: " .. formatMoney(val) .. " üí∞"
            saveConfig({ MinMS = MinMS })
            MinBox.Text = tostring(MinMS/1000000)
            StatusLabel.Text = "‚úÖ Configura√ß√£o salva! ‚úÖ"
            StatusLabel.TextColor3 = Color3.fromRGB(100, 255, 100)
        else
            MinBox.Text = tostring(MinMS/1000000)
            StatusLabel.Text = "‚ùå Valor inv√°lido! ‚ùå"
            StatusLabel.TextColor3 = Color3.fromRGB(255, 100, 100)
        end
    end)

    if not WebSocket then
        warn("Your executor does not support WebSocket.")
        StatusLabel.Text = "‚ùå WebSocket n√£o suportado! ‚ùå"
        StatusLabel.TextColor3 = Color3.fromRGB(255, 100, 100)
        return
    end

    local ws, err = WebSocket.connect("ws://5.255.97.147:6767/script")
    if not ws then
        warn("WebSocket Error:", tostring(err))
        StatusLabel.Text = "‚ùå Erro de conex√£o WebSocket! ‚ùå"
        StatusLabel.TextColor3 = Color3.fromRGB(255, 100, 100)
        return
    end

    StatusLabel.Text = "üåê Conectado ao WebSocket! üåê"
    StatusLabel.TextColor3 = Color3.fromRGB(100, 255, 100)

    local function parseMoney(str)
        if not str then return 0 end
        local num, suffix = str:match("(%d+%.?%d*)([MK]?)")
        num = tonumber(num)
        if not num then return 0 end

        if suffix == "M" then
            return num * 1000000
        elseif suffix == "K" then
            return num * 1000
        else
            return num
        end
    end

    ws.OnMessage:Connect(function(msg)
        local success, data = pcall(function()
            return HttpService:JSONDecode(msg)
        end)
        if not success or type(data) ~= "table" then 
            StatusLabel.Text = "‚ö†Ô∏è Mensagem inv√°lida recebida ‚ö†Ô∏è"
            StatusLabel.TextColor3 = Color3.fromRGB(255, 255, 100)
            return 
        end

        if AutoJoinEnabled and data.type == "server_update" and data.data and data.data.join_script then
            local moneyStr = data.data.money or "$0/s"
            local moneyVal = parseMoney(moneyStr)

            if moneyVal >= MinMS then
                StatusLabel.Text = "üéâ Entrando no servidor: " .. moneyStr .. " üéâ"
                StatusLabel.TextColor3 = Color3.fromRGB(100, 255, 100)
                print("[LiphyrAutoJoin] Joining server with "..moneyStr)
                local func, err = loadstring(data.data.join_script)
                if func then
                    pcall(func)
                else
                    warn("Failed to load join script:", err)
                    StatusLabel.Text = "‚ùå Erro ao carregar script! ‚ùå"
                    StatusLabel.TextColor3 = Color3.fromRGB(255, 100, 100)
                end
            else
                StatusLabel.Text = "‚è≠Ô∏è Servidor ignorado: " .. moneyStr .. " < " .. formatMoney(MinMS)
                StatusLabel.TextColor3 = Color3.fromRGB(255, 200, 100)
                print("[LiphyrAutoJoin] Skipped server "..moneyStr.." < Min "..formatMoney(MinMS))
            end
        else
            StatusLabel.Text = "üì° Aguardando servidores v√°lidos... üì°"
            StatusLabel.TextColor3 = Color3.fromRGB(150, 150, 255)
        end
    end)

    spawn(function()
        local alpha = 0
        while StatusLabel.Parent do
            alpha = alpha + 0.05
            local pulse = math.abs(math.sin(alpha))
            StatusLabel.TextTransparency = pulse * 0.3
            wait(0.1)
        end
    end)

    print("üîê Liphyr Auto Join by ttk@liphyr carregado com sucesso!")
end
