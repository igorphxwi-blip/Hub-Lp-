-- Aba de Scripts
local ScriptsTab = Window:CreateTab("Scripts | LPX", 0)
ScriptsTab:CreateParagraph({Title = "Note", Content = "The hub is open-sourced so you can check the hub for loggers!"})

local function createScriptButton(name, url)
    ScriptsTab:CreateButton({
        Name = name,
        Callback = function()
            loadstring(game:HttpGet(url))()
            Rayfield:Notify({
                Title = name.." Ativado",
                Content = "Script executado ‚úÖ",
                Duration = 5,
                Image = 74306962782905
            })
        end
    })
end

createScriptButton("MirandaTween+","https://pastefy.app/TnWWJ4NS/raw")
createScriptButton("Auto Secret","https://tcscripts.discloud.app/scripts/autosecret")
createScriptButton("Spawner Brainrot","https://gitlab.com/traxscriptss/traxscriptss/-/raw/main/visual2.lua")
createScriptButton("Chilli Hub","https://raw.githubusercontent.com/tienkhanh1/spicy/main/Chilli.lua")
