-- Pegar as keys do GitHub
local rawKeys = game:HttpGet("https://raw.githubusercontent.com/igorphxwi-blip/Hub-Lp-/refs/heads/main/keys.txt")
local keysArray = {}
for line in rawKeys:gmatch("[^\r\n]+") do
    table.insert(keysArray, line)
end

-- Carregar Rayfield
local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

