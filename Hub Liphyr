-- Aba Player
local PlayerTab = Window:CreateTab("Player", 0)

local ESPEnabled = false
local InfiniteJump = false
local GodMode = false
local SpeedEnabled = false
local SpeedValue = 16 -- Valor padr√£o

-- Sauda√ß√£o autom√°tica
Rayfield:Notify({
    Title = "Ol√° Cliente "..game.Players.LocalPlayer.Name,
    Content = "Bem-vindo!",
    Duration = 5,
    Image = 74306962782905
})

-- ESP Player
PlayerTab:CreateToggle({
    Name = "ESP Player",
    CurrentValue = false,
    Callback = function(value)
        ESPEnabled = value
        for i, plr in pairs(game.Players:GetPlayers()) do
            if plr ~= game.Players.LocalPlayer and plr.Character and plr.Character:FindFirstChild("HumanoidRootPart") then
                local highlight = plr.Character:FindFirstChild("ESPHighlight")
                if value then
                    if not highlight then
                        highlight = Instance.new("Highlight")
                        highlight.Name = "ESPHighlight"
                        highlight.Adornee = plr.Character
                        highlight.FillColor = Color3.fromRGB(111,0,255)
                        highlight.OutlineColor = Color3.fromRGB(60,0,100)
                        highlight.Parent = plr.Character
                    end
                else
                    if highlight then
                        highlight:Destroy()
                    end
                end
            end
        end
        Rayfield:Notify({
            Title = "ESP Player",
            Content = value and "Ativado ‚úÖ" or "Desativado ‚ùå",
            Duration = 5,
            Image = 74306962782905
        })
    end
})

-- God Mode REAL
PlayerTab:CreateToggle({
    Name = "God Mode",
    CurrentValue = false,
    Callback = function(value)
        GodMode = value
        Rayfield:Notify({
            Title = "God Mode",
            Content = value and "Ativado ‚úÖ" or "Desativado ‚ùå",
            Duration = 5,
            Image = 74306962782905
        })
    end
})

game:GetService("RunService").Heartbeat:Connect(function()
    local plr = game.Players.LocalPlayer
    if plr.Character and plr.Character:FindFirstChild("Humanoid") then
        local hum = plr.Character.Humanoid
        if GodMode then
            hum.Health = hum.MaxHealth
        end
        if SpeedEnabled then
            hum.WalkSpeed = SpeedValue
        end
    end
end)

-- Infinite Jump
PlayerTab:CreateToggle({
    Name = "Infinite Jump",
    CurrentValue = false,
    Callback = function(value)
        InfiniteJump = value
        Rayfield:Notify({
            Title = "Infinite Jump",
            Content = value and "Ativado ‚úÖ" or "Desativado ‚ùå",
            Duration = 5,
            Image = 74306962782905
        })
    end
})

game:GetService("UserInputService").JumpRequest:Connect(function()
    if InfiniteJump then
        local plr = game.Players.LocalPlayer
        if plr.Character and plr.Character:FindFirstChild("Humanoid") then
            plr.Character.Humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
        end
    end
end)

-- Speed regul√°vel
PlayerTab:CreateSlider({
    Name = "Speed Player",
    Range = {16, 100},
    Increment = 1,
    Suffix = " WalkSpeed",
    CurrentValue = 16,
    Flag = "SpeedSlider",
    Callback = function(value)
        SpeedValue = value
        SpeedEnabled = value > 16
        Rayfield:Notify({
            Title = "Speed",
            Content = SpeedEnabled and ("Ativado ‚úÖ | "..SpeedValue) or "Desativado ‚ùå",
            Duration = 5,
            Image = 74306962782905
        })
    end
})

