-- Efeito Rainbow na borda do KeyFrame
spawn(function()
    local hue = 0
    while KeyFrame.Parent do
        hue = (hue + 2) % 360
        local r = math.floor(255 * (1 + math.sin(math.rad(hue))) / 2)
        local g = math.floor(255 * (1 + math.sin(math.rad(hue + 120))) / 2)
        local b = math.floor(255 * (1 + math.sin(math.rad(hue + 240))) / 2)
        KeyFrame.BorderColor3 = Color3.fromRGB(r, g, b)
        wait(0.1)
    end
end)

