-- LIPHYR AUTO JOIN - Sistema de Key by ttk@liphyr
local Players = game:GetService("Players")
local HttpService = game:GetService("HttpService")
local TweenService = game:GetService("TweenService")
local RunService = game:GetService("RunService")

local player = Players.LocalPlayer

-- Configura√ß√£o da Key (modifique com sua URL do GitHub)
local KEY_URL = "https://raw.githubusercontent.com/SEU_USUARIO/SEU_REPO/main/keys.json"
local HWID = game:GetService("RbxAnalyticsService"):GetClientId()

-- Arquivo de configura√ß√£o da key
local keyFile = "liphyr_key_" .. HWID .. ".json"

-- Fun√ß√£o para verificar se a key j√° foi salva
local function loadSavedKey()
    if isfile and isfile(keyFile) then
        local success, data = pcall(function()
            return HttpService:JSONDecode(readfile(keyFile))
        end)
        if success and type(data) == "table" and data.key then
            return data.key
        end
    end
    return nil
end

-- Fun√ß√£o para salvar a key
local function saveKey(key)
    if writefile then
        writefile(keyFile, HttpService:JSONEncode({key = key, hwid = HWID, timestamp = os.time()}))
    end
end

-- Fun√ß√£o para validar key no GitHub
local function validateKey(inputKey)
    local success, response = pcall(function()
        return game:HttpGet(KEY_URL, true)
    end)
    
    if not success then
        return false, "Erro ao conectar com o servidor de keys"
    end
    
    local keyData = HttpService:JSONDecode(response)
    
    -- Verificar se a key existe na lista
    if keyData.valid_keys then
        for _, keyInfo in pairs(keyData.valid_keys) do
            if keyInfo.key == inputKey then
                -- Verificar se n√£o expirou (se tiver data de expira√ß√£o)
                if keyInfo.expires and os.time() > keyInfo.expires then
                    return false, "Key expirada"
                end
                return true, "Key v√°lida!"
            end
        end
    end
    
    return false, "Key inv√°lida"
end

